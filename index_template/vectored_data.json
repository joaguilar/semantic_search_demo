PUT https://localhost:9200/_template/vectortemplate
{
  "index_patterns": ["vector-*"],
    "settings": {
      "number_of_shards": 2
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
          "sentence-vector": {
              "type": "dense_vector",
              "dims": 768,
              "index": true,
              "similarity": "l2_norm"
            },
        "my_id":{
          "type":"keyword"
        },
        "title": {
          "type": "keyword"
        },
        "content":{
          "type":"text"
        },
        "sentence":{
          "type": "keyword"
        },
        "articles_sentence":{
          "type": "join",
          "relations":{
              "articles":"sentence"
          }
        }
      }
    },
    "aliases": {
      "demo": { }
    },
  "version": 1,
  "_meta": {
    "description": "Documents and Sentence indices"
  }
}
